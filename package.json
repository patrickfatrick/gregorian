{
  "name": "gregorian",
  "version": "2.0.4",
  "description": "A Javascript micro library for converting and displaying dates",
  "main": "dist/gregorian.js",
  "format": "es6",
  "engines": {
    "node": ">= 4.0.0"
  },
  "scripts": {
    "build": "standard './src/**/*.js' | snazzy && gulp",
    "test": "TZ=MST node_modules/.bin/nyc --reporter=lcov --reporter=text-lcov ava --tap | node_modules/.bin/faucet",
    "report": "node_modules/.bin/nyc report"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/patrickfatrick/gregorian"
  },
  "keywords": [
    "javascript",
    "dates"
  ],
  "author": "Patrick Fricano <patrick.fricano@icloud.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-core": "^6.24.0",
    "babelify": "^7.2.0",
    "browserify": "^14.3.0",
    "codecov": "^2.1.0",
    "codecov.io": "^0.1.6",
    "faucet": "0.0.1",
    "gulp": "^3.9.1",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.7",
    "nyc": "^10.2.0",
    "require-dir": "^0.3.0",
    "sinon": "^2.1.0",
    "snazzy": "^6.0.0",
    "standard": "^10.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "babel-preset-env": "^1.3.2"
  }
}
